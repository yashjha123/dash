{
  "name": "dash-renderer",
  "version": "1.15.4",
  "description": "render dash components in react",
  "main": "build/dash_renderer.min.js",
  "scripts": {
    "prepublishOnly": "rm -rf lib && babel src --extensions=\".ts,.tsx,.js,.jsx\" --out-dir lib --copy-files",
    "private::format.eslint": "eslint --quiet --fix src tests",
    "private::format.prettier": "prettier --write \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "private::lint.eslint": "eslint src tests",
    "private::lint.prettier": "prettier \"{src,tests}/**/*.{js,jsx,ts,tsx}\" --list-different",
    "build:js": "webpack",
    "build:dev": "webpack --watch",
    "build:local": "renderer build local",
    "build": "renderer build && npm run prepublishOnly",
    "postbuild": "es-check es2015 ../deps/*.js build/*.js",
    "test": "karma start karma.conf.js --single-run",
    "format": "run-s private::format.*",
    "lint": "run-s private::lint.*"
  },
  "author": "chriddyp",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@plotly/dash-component-plugins": "^1.2.3",
    "cookie": "^0.5.0",
    "cytoscape": "^3.25.0",
    "cytoscape-dagre": "^2.5.0",
    "cytoscape-fcose": "^2.2.0",
    "dependency-graph": "^0.11.0",
    "fast-isnumeric": "^1.1.4",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prop-types": "15.8.1",
    "ramda": "^0.29.0",
    "react": "16.14.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "16.14.0",
    "react-json-tree": "^0.17.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.2",
    "regenerator-runtime": "^0.13.11",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@plotly/prettier-config-dash": "0.0.1",
    "@svgr/webpack": "^8.0.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/ramda": "0.29.1",
    "@types/react": "^16.14.8",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "css-loader": "^6.7.4",
    "es-check": "^7.1.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "files": [
    "build/*{.js,.map}",
    "/lib/**"
  ],
  "prettier": "@plotly/prettier-config-dash",
  "browserslist": [
    "last 8 years and not dead"
  ]
}
